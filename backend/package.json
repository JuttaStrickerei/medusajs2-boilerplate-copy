    "react-dom": "^18.3.1",
    "react-email": "^3.0.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vite": "^5.4.21"
  },
  "peerDependencies": {
    "zod": "3.22.4"
  },
  "pnpm": {
    "overrides": {
      "zod": "3.22.4"
    }
  },
  "packageManager": "pnpm@9.10.0",
  "engines": {
    "node": "22.x",
    "npm": "10.x"
  }
