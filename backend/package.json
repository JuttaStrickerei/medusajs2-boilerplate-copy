    "express": "^4.22.1",
    "medusajs-launch-utils": "^0.0.10",
    "minio": "^8.0.6",
    "pdfmake": "^0.2.20",
    "pg": "^8.16.3",
    "react-hook-form": "^7.69.0",
    "resend": "4.0.1",
    "ulid": "^2.4.0",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@medusajs/test-utils": "latest",
    "@mikro-orm/cli": "6.4.3",
    "@swc/core": "1.5.7",
    "@swc/jest": "^0.2.39",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.27",
    "@types/pdfmake": "^0.2.12",
